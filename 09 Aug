//CODEFORCES PROBLEM->BEAUTIFUL MATRIX

import java.util.*;
public class Main{
    
    public static void main (String args[]){
        Scanner sc=new Scanner(System.in);
        int center=0;

        for(int i=0;i<5;i++){
            for(int j=0;j<5;j++){
                if(sc.nextInt()==1){
                    center=Math.abs(i-2)+Math.abs(j-2);
                }
            }
        }
        System.out.println(center);
    }
}