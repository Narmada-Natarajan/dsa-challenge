//CODEFORCES PROBLEM -> FOOTBALL

import java.util.*;
public class Main{

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine().trim();
        
        if(s.contains("0000000") || s.contains("1111111")){ 
             System.out.print("YES");
        }
        else{
            System.out.print("NO");
        }
    }
}