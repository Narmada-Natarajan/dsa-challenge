//CODEFORCES PROBLEM -> DOMINO PILING

import java.util.*;

public class Main12{
    public static void main (String args[]){

        Scanner sc=new Scanner(System.in);
        Long m=sc.nextLong();
        Long n=sc.nextLong();

        System.out.println((m * n)/2);//max dominoes in two sq
    }
}
