//CODEFORCES PROBLEM -> PANORAMIX'S PREDICTION

import java.util.*;
public class Main{
    
    public static void main (String[]args){
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        int y=sc.nextInt();
        
        int next=x+1;
        while(!prime(next))next++;
        System.out.println(next==y?"YES":"NO");
        }

    static boolean prime(int n){
        if(n < 2) return false;
        for(int i=2;i*i<=n;i++) 
            if(n%i==0) return false;
        return true;
    }
}