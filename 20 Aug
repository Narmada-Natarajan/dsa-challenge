//CODEFORCES PROBLEM -> DRINK 

import java.util.*;
public class Main20{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        double sum=0.0;

        for(int i=0;i<n;i++) {
            sum+=sc.nextDouble();
        }
        double result=sum/n;
        System.out.printf("%.12f%n",result);
    }
}
